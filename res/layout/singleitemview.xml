<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
  
    >
    <RelativeLayout 
	    
	     android:layout_width="fill_parent"
        android:layout_height="55dp"
        android:background="#f26522">

	    
	    
	    
		<Button
		    android:id="@+id/menubutton"
		    android:layout_width="55dp"
		    android:layout_height="55dp"
		    
		    android:onClick="back"
		    android:background="@drawable/backbutt" />

		

		<Button
		    android:id="@+id/optionbutton"
		    android:layout_width="55dp"
		    android:layout_height="wrap_content"
		    android:layout_alignParentRight="true"
		    android:layout_alignParentTop="true"
		    
		    android:background="@drawable/sharebutt" />

		<com.androidbegin.jsonparsetutorial.Coustxt		    
		    android:id="@+id/header"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerHorizontal="true"
		    android:layout_centerVertical="true"
		    android:textColor="#ffffff"
		    android:textSize="25dp"
		    android:textStyle="bold"
		    android:text="Recent" />

	</RelativeLayout>
  
 <!--      <com.nirhart.parallaxscroll.views.ParallaxScrollView  -->
     <com.nirhart.parallaxscroll.views.ParallaxScrollView
 
     android:id="@+id/para"
            
             android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:parallax_factor="1.9" >
    
         <LinearLayout
              android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

             <com.androidbegin.jsonparsetutorial.NewImageView
                 android:id="@+id/flag"
                 android:layout_width="fill_parent"
                 android:layout_height="wrap_content"
                 android:layout_weight="1"
                 android:adjustViewBounds="true"
                 android:gravity="center"
                 android:minHeight="500dp"
                 android:text="PARALLAXED"
                 android:textSize="50sp"
                  android:background="#cccccc"
                 tools:ignore="HardcodedText" />
           
            <LinearLayout
                
                
                  android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:background="#ffffff"
                
                
                 >
          <com.androidbegin.jsonparsetutorial.Coustxt
        android:id="@+id/country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
         android:layout_marginRight="15dp" android:layout_marginLeft="15dp"
         android:background="#ffffff"
        android:text="counter"
        android:textSize="35dp"
        android:textStyle="bold" />
    
          
          
            <com.androidbegin.jsonparsetutorial.Newtxt
        android:id="@+id/author"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="By LustyPrince"
        android:layout_gravity="left" 
           android:layout_marginTop="20dp" 
              android:layout_marginLeft="15dp" 
        android:textSize="20dp"
        android:textColor="#a1a1a1"
     
    />
          
        <com.androidbegin.jsonparsetutorial.Newtxt
        android:id="@+id/rank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="counter rank"
        android:layout_gravity="left" 
        android:layout_marginBottom="15dp" 
           android:layout_marginLeft="15dp" 
        android:textColor="#a1a1a1"
        android:textStyle="italic"
    />
          

   

 

  
<com.androidbegin.jsonparsetutorial.Newtxt
        android:id="@+id/population"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="counterpopulation"
        android:textSize="20dp"
        android:textColor="#555555"
        android:layout_margin="15dp" 
        android:layout_gravity="right"/>

<com.androidbegin.jsonparsetutorial.Newtxt
    android:id="@+id/lin"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:text="TextView"
    android:layout_margin="10dp"
    android:textColor="#605ca8"
     android:layout_marginBottom="20dp"
 />


<LinearLayout 
    
    
      android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:gravity="right"
    
    >
    
   
    <Button
        android:id="@+id/comsub"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/commbutt"
         />
    </LinearLayout>

    <!--  
  
      
      
     
       
        <ListView
           android:id="@+id/comview"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
            >
            
      </ListView>
      
      
       
       -->
   
  

  

  


        <it.sephiroth.android.library.widget.HListView
            android:id="@+id/gridView1"
            android:layout_width="match_parent"
            android:layout_height="150dp"
 android:background="#cccccc"
            android:divider="@null"
            android:dividerHeight="0dp"
            
             />
        
            
        
            
  

    </LinearLayout>
    
    </LinearLayout>
    

   
</com.nirhart.parallaxscroll.views.ParallaxScrollView></LinearLayout>
